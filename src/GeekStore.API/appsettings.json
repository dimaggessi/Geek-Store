{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5005"
      }
    }
  },
  "ConnectionStrings": {
    "SqlServer": "${ConnectionStrings__SqlServer}",
    "Redis": "${ConnectionStrings__Redis}"
  },
  "StripeKeys": {
    "Publishable": "${StripeKeys__Publishable}",
    "Secret": "${StripeKeys__Secret}",
    "WhSecret": "${StripeKeys__WhSecret}"
  },
  "MelhorEnvio": {
    "PostalCode": "${MelhorEnvio__PostalCode}",
    "Email": "${MelhorEnvio__Email}",
    "Url": "${MelhorEnvio__Url}",
    "Token": "${MelhorEnvio__Token}",
    "ChosenServices": "1,2"
  },
  "Admin": {
    "Password": "${Admin__Password}"
  }
}